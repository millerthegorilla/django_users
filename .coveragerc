[run]
omit = django_users/tests/*
	   *settings.py
	   *__init__.py
	   *wsgi.py
	   *asgi.py
	   *manage.py
	   *setup.py
	   *fields.py
	   *tests.py
	   venv/*
	   *testapp/*
	   *build/lib/*
	   django_users.egg-info/*
	   django_users/context_processors.py

source = .

branch = True

plugins = django_coverage_plugin


[report]
fail_under = 100
show_missing = True
skip_covered = True